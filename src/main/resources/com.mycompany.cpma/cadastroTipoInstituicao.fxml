<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/23.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.CadastroTipoInstituicaoController">
    <children>
        <Text layoutX="20.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Novo Tipo de Instituição">
            <font>
                <Font name="System Bold" size="18.0"/>
            </font>
        </Text>
        <Label layoutX="20.0" layoutY="60.0" text="Tipo" />
        <TextField fx:id="campoTipo" layoutX="20.0" layoutY="80.0" prefWidth="360.0" />

        <Button fx:id="btnSalvar" layoutX="100.0" layoutY="130.0" text="Salvar" prefWidth="100.0"/>
        <Button fx:id="btnCancelar" layoutX="210.0" layoutY="130.0" text="Cancelar" prefWidth="100.0"/>
    </children>
</AnchorPane>
